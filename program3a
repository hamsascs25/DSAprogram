#include <stdio.h>
#define MAX 5

int q[MAX];
int front = -1, rear = -1;

void insert(int item)
{
    if (rear == MAX - 1)
        printf("\nQueue Overflow");
    else
    {
        if (front == -1)
            front = 0;
        q[++rear] = item;
    }
}

void delete()
{
    if (front == -1 || front > rear)
        printf("\nQueue Empty");
    else
        printf("\nDeleted element is %d", q[front++]);
}

void display()
{
    int i;
    if (front == -1 || front > rear)
        printf("\nQueue Empty");
    else
    {
        printf("\nQueue elements are: ");
        for (i = front; i <= rear; i++)
            printf("%d ", q[i]);
    }
}

int main()
{
    int choice, item;
    while (1)
    {

        printf("\n\n1. Insert");
        printf("\n2. Delete");
        printf("\n3. Display");
        printf("\n4. Exit");
        printf("\nEnter your choice: ");
        scanf("%d", &choice);

        switch (choice)
        {
        case 1:
            printf("\nEnter the element to insert: ");
            scanf("%d", &item);
            insert(item);
            break;
        case 2:
            delete();
            break;
        case 3:
            display();
            break;
        case 4:
            return 0;
        default:
            printf("\nInvalid choice");
        }
    }
}
