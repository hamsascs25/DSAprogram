#include <stdio.h>
#include <ctype.h>

char stack[100];
int top = -1;

void push(char c)
{
    stack[++top] = c;
}

char pop()
{
    return stack[top--];
}

int main()
{
    char infix[100], postfix[100];
    int i = 0, k = 0;
    char ch;

    scanf("%s", infix);

    while ((ch = infix[i++]) != '\0')
    {
        if (isalnum(ch))
            postfix[k++] = ch;
        else if (ch == '+' || ch == '-' || ch == '*' || ch == '/')
            push(ch);
        else if (ch == ')')
            postfix[k++] = pop();
    }

    postfix[k] = '\0';
    printf("%s", postfix);

    return 0;
}
